pipeline{
    agent any
    stages{
        stage ('code analysis'){
            steps {
                sh 'echo "sonar running"'
            }
        }
        stage ('code testing'){
            steps {
                sh 'echo "testing"'
            }
        }
        stage ('build'){
            steps {
                sh 'echo "building application"'
            }
        }
        stage ('deploy'){
            steps {
                sh 'echo "application deployed to prod server"'
            }
        }
    }
}
